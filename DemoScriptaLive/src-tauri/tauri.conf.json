{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "identifier": "com.scripta.live",
  "build": {
    "beforeBuildCommand": "elm make src/MainTauri.elm --output=assets/main-tauri.js",
    "beforeDevCommand": "elm make src/MainTauri.elm --output=assets/main-tauri.js",
    "frontendDist": "../assets"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Scripta Desktop v0.3a",
  "version": "0.2.0",
  "app": {
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true
    },
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Scripta Desktop v0.3a",
        "label": "main",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "url": "index-tauri.html"
      }
    ]
  }
}